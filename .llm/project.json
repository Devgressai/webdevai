    "lib": {
      "description": "Utility functions and helpers",
      "key_files": [
        "lib/cities.ts - City data configuration",
        "lib/industries.ts - Industry data",
        "lib/clean-schema-generator.ts - JSON-LD schema generation"
      ]
    },
    "public": {
      "description": "Static assets and configuration files",
      "key_files": [
        "public/robots.txt - Search engine crawl instructions",
        "public/sitemap.xml - Generated dynamically",
        "public/images/ - Static images"
      ]
    }
